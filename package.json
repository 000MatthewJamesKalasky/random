{
  "name": "random",
  "version": "2.2.0",
  "description": "Seedable random number generator supporting many common distributions.",
  "main": "dist/random.js",
  "module": "dist/random.js",
  "types":"dist/random.d.ts",
  "repository": "transitive-bullshit/random",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "reveal": true,
  "scripts": {
    "start": "yarn build:watch",
    "docs": "update-markdown-jsdoc -f dist/random.js --shallow",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prebuild": "yarn run clean",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "test": "ava -v && standard",
    "seed-test": "ava --match='*seed*'"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "rng",
    "prng",
    "stats",
    "d3-random",
    "seedrandom",
    "distribution",
    "pseudorandom",
    "uniform",
    "normal",
    "gaussian",
    "lognormal",
    "poisson",
    "exponential",
    "irwinhall",
    "bates"
  ],
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^14.14.31",
    "@types/seedrandom": "^2.4.28",
    "ava": "^3.5.0",
    "del-cli": "^1.1.0",
    "dev-cli": "^0.0.0",
    "standard": "^14.3.3",
    "ts-node": "^9.1.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.1.5",
    "update-markdown-jsdoc": "^1.0.11"
  },
  "dependencies": {
    "ow": "^0.4.0",
    "ow-lite": "^0.0.2",
    "seedrandom": "^3.0.5"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "ava": {
    "extensions": [
			"ts"
		],
    "require": [
      "ts-node/register"
    ]
  }
}
