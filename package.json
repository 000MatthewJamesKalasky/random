{
  "name": "random",
  "version": "2.0.0",
  "description": "Seedable random number generator supporting many common distributions.",
  "main": "index.js",
  "module": "src/random.js",
  "browser": "dist/random.js",
  "repository": "transitive-bullshit/random",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "scripts": {
    "docs": "update-markdown-jsdoc -f src/random.js --shallow",
    "prebuild": "yarn run clean",
    "build": "rollup -c",
    "buildWatch": "rollup -c -w",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "test": "ava -v && standard"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "rng",
    "prng",
    "stats",
    "d3-random",
    "seedrandom",
    "distribution",
    "pseudorandom",
    "uniform",
    "normal",
    "gaussian",
    "lognormal",
    "poisson",
    "exponential",
    "irwinhall",
    "bates"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^1.1.0",
    "esm": "^3.0.48",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "standard": "^11.0.0",
    "update-markdown-jsdoc": "^1.0.2"
  },
  "dependencies": {
    "lru-cache": "^4.1.3",
    "ow": "^0.4.0",
    "seedrandom": "^2.4.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers",
      "transform-runtime"
    ]
  }
}
